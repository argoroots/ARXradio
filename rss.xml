<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
    <channel>
        <link>http://raadio.arx.ee/rss{{ show }}</link>
        <title>ETV/ETV2 feed</title>
        <atom:link href="http://raadio.arx.ee/rss{{ show }}" rel="self" type="application/rss+xml" />
        <description>ETV/ETV2 feed</description>
            {% for i in items %}
            <item>
                <title>{{ i.title }}</title>
                <pubDate>{{ i.sdate }}</pubDate>
                <link>{{ i.url }}</link>
                <guid isPermaLink="false">{{ i.url }}</guid>
                <enclosure url="{{ i.url }}" length="0" type="video/x-m4v" />
                <description>{{ i.show }}</description>
            </item>
            {% endfor %}
    </channel>
</rss>
